
    <!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Photo Gallery
                    
                </h1>
                <ol class="breadcrumb">
                    <li><a href="/">Home</a>
                    </li>
                    <li class="active">All photos</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <!-- Projects Row -->
        <div class="row">  
            <hr>
            <% if (myAccountPhotoList.length === 0) { %>
            <h2> You Don't Have photos Yet </h2>
            <% } else { %>
              <h2> Your Photos </h2>


            <% myAccountPhotoList.forEach((onePhoto) => { %>
            <div class="col-md-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-check"></i> <%= onePhoto.photoTitle%></h4>
                        </div>
                        <div class="col-md-12 col-sm-6">
                            <a href="/photo/<%= onePhoto._id %>">
                            <img class="img-responsive img-portfolio img-hover" src='<%=onePhoto.imageUrl%>' alt="">
                            </a>
                        </div>
                    </div>
            </div>
            <% }) %>
        </div>
    </div>
    <% } %>
<a class="btn btn-primary" href="/photo/new">Add a new photo</i></a>

  


