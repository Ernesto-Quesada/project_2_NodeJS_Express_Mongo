 <!-- Header Carousel -->
    <header id="myCarousel" class="carousel slide">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
         <% photoList.forEach((onePhoto,index) => { %>
            <div class="item <% if (index === 0) { %> active <% } %>">
        
                <div class="fill" style="background-image:url('<%= onePhoto.imageUrl %>');"></div>
                <div class="carousel-caption">
                    <h2><%=onePhoto.photoTitle %></h2>
                </div>
            </div>
            <% }); %>
           
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="icon-prev"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="icon-next"></span>
        </a>
    </header>

    <!-- Page Content -->
    <div class="container">
    <!-- Marketing Icons Section -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    Welcome to <%= title%>
                </h1>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-check"></i> Great Images deserve to be Printed</h4>
                    </div>
                    <div class="panel-body">
                        <p>Ever wanted to print in big format that photo that you love but never had the time? This is the perfect way to do it. Just upload a picture in high quality, if people really like it, we will print it for you. </p>
                        <a href="/signup"  class="btn btn-default">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-gift"></i> Rate and win... is FREE</h4>
                    </div>
                    <div class="panel-body">
                        <p>Why rating has to be boring. Win a high quality printed photography just for rating</p>
                        <a href="/signup" class="btn btn-default">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-compass"></i> Easy to Use</h4>
                    </div>
                    <div class="panel-body">
                        <p>Just upload your image.If your image gets to the featured section after been rated by other users. We will print it and send it to you as a gift</p>
                        <a href="/signup" class="btn btn-default">Learn More</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->

        <!-- Features Section -->
        <div class="row">
            
        </div>
        <div class="row">
            <div class="col-lg-12">
                <h2 class="page-header">Featured Photo</h2>
            </div>
            <div class="col-md-6">
                <p><%= photoList[1].photoTitle%></p>

            </div>
            <div class="col-md-6">
                <img class="img-responsive" src="<%=photoList[1].imageUrl%>" alt="">
            </div>
        </div>
        <!-- /.row -->
