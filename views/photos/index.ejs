    <!-- Page Content -->
<div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Photo Gallery
                    
                </h1>
                <ol class="breadcrumb">
                    <li><a href="/">Home</a>
                    </li>
                    <li class="active">All photos</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
        


        <div class="row"> 
             <% photoList.forEach((onePhoto) => { %>
                <div class="col-md-4">
                    <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4> <%= onePhoto.photoTitle%></h4>
                    </div>
                    <div class="col-md-12 col-sm-6">
                            <a href="/photo/<%= onePhoto._id %>">
                            <img class="img-responsive img-portfolio img-hover" src='<%=onePhoto.imageUrl%>' alt="">
                            </a>
                            
                    </div>
                    </div>
                </div>
            <% }) %>
        </div>
</div>
  


