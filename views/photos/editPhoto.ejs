<h2>Edit photo</h2>
<form method="post"  action="/photo/<%= photo._id %> ">
    <label for="photoTitle">Title</label>
    <input id="photoTitle" type="text" name="photoTitle" value="<%= photo.photoTitle%>">


        <label for="propellers">Propellers</label>
    <input id="propellers" type="text" name="propellers" value="<%= photo.propellers %>">
    
<label for="description">Description</label>
    <textarea id="description" name="description"><%= photo.description%></textarea>
    <br>
<button type="submit">submit</button>

</form>